# Change the ansible_ssh_hosts to the relevant ip or fully qualified dns names
[docker-registry-servers]
registry1 ansible_ssh_host=137.58.70.157

[docker-registry-servers:vars]
ansible_ssh_user=esoutri
ansible_ssh_private_key_file=~/.ssh/id_rsa
ansible_connection=local

[shipyard-servers]
hostserver1 ansible_ssh_host=137.58.70.157

[shipyard-servers:vars]
ansible_ssh_user=esoutri
ansible_ssh_private_key_file=~/.ssh/id_rsa
ansible_connection=local

[shipyard-agents]
hostnode1 ansible_ssh_host=137.58.70.157

[shipyard-agents:vars]
ansible_ssh_user=esoutri
ansible_ssh_private_key_file=~/.ssh/id_rsa
ansible_connection=local
